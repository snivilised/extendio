{
  "Store": {
    "Subscription": 1,
    "DoExtend": true,
    "WithMetrics": false,
    "Behaviours": {
      "SubPath": {
        "KeepTrailingSep": true
      },
      "Sort": {
        "IsCaseSensitive": false,
        "DirectoryEntryOrder": 0
      },
      "Listen": {
        "InclusiveStart": true,
        "InclusiveStop": false
      }
    },
    "FilterDefs": {
      "Current": {
        "Type": 2,
        "Description": "items with .flac suffix",
        "Source": "*.flac",
        "Scope": 4,
        "Negate": false,
        "IfNotApplicable": true
      },
      "Children": {
        "Type": 1,
        "Description": "jpg files",
        "Source": "\\.jpg$",
        "Negate": true
      }
    }
  },
  "Active": {
    "Root": "fake-path-override-me-in-test/",
    "Listen": 1
  }
}